{
    "ell2a_integration": {
        "enabled": true,
        "sync_interval": 60,
        "metrics": {
            "enabled": true,
            "collection_interval": 30,
            "retention_days": 30
        },
        "monitoring": {
            "enabled": true,
            "health_check_interval": 300,
            "alert_thresholds": {
                "error_rate": 0.1,
                "latency_ms": 1000,
                "memory_mb": 1024,
                "validation_score": 0.7
            }
        },
        "visualization": {
            "enabled": true,
            "update_interval": 60,
            "dashboard_config": {
                "default_timespan": "1h",
                "refresh_rate": 30,
                "max_datapoints": 1000
            }
        }
    }
}
